<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RPSClinicalApp.ReceptionPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="FirstName"></Label>
            <Label x:Name="LastName"></Label>
            <Label x:Name="Email"></Label>
            <Label x:Name="Phone"></Label>
            <Label x:Name="DOB"></Label>
            <StackLayout>
                <Button Text="ShareRequest" Clicked="Share_Clicked"/>
            </StackLayout>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>